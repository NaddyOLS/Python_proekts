import random
balls1=0
balls2=0
balls3=0
polz1=int(input())
polz2=int(input())
programm1=random.randint(1,3)
programm2=random.randint(1,3)
print(f"Компьютер спрятался в комнате: {programm1}")
print(f"Компьютер ищет тебя в комнате: {programm2}")
if polz2 == programm1:
    balls1 +=1
if programm2 == polz1:
    balls2 +=1
if balls1 > balls2:
    print("Великий игрок, ты выиграл!")
elif balls2 > balls1:
    print("Великий игрок зазевался и случайно уступил компу")
elif balls1 == balls2:
    print("Великий игрок сжалился и допустил ничью")
